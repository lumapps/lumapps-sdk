#!/home/aurelien/Lumapps/sdk-lumapps-fork/env36/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'lumapps-sdk','console_scripts','lac'
__requires__ = 'lumapps-sdk'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('lumapps-sdk', 'console_scripts', 'lac')()
    )
